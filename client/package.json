        "vue-color": "^2.7.0",
        "vue-i18n": "^8.18.1",
        "vue-loading-overlay": "^3.3.0",
        "vue-markdown": "^2.2.4",
        "vue-property-decorator": "^8.3.0",
        "vue-router": "^3.1.3",
        "vue-slider-component": "^3.0.42",
        "vuedraggable": "^2.23.2",
        "vuex": "^3.0.1",
        "vuex-module-decorators": "^0.11.0"
