{
    "name": "client",
    "version": "2023.3.0",
    "private": true,
    "scripts": {
        "dev": "vite",
        "serve": "vite preview",
        "clean": "rimraf ../server/static/vite/",
        "build:prep": "npm run clean",
        "build": "npm run build:prep && vite build && mv ../server/index.html ../server/templates/index.html",
        "build:ci": "npm run build:prep && npm run vue-tsc && vite build --outDir dist",
        "lint": "eslint src --fix --ext .ts,.vue",
        "lint:ci": "CI=1 eslint src --no-fix --ext .ts,.vue",
        "vue-tsc": "vue-tsc --noEmit",
        "tsc": "tsc --noEmit",
        "test": "vitest",
        "coverage": "vitest run --coverage"
    },
    "dependencies": {
        "@babylonjs/materials": "^4.2.2",
        "@fortawesome/fontawesome-svg-core": "^6.3.0",
        "@fortawesome/free-brands-svg-icons": "^6.3.0",
        "@fortawesome/free-regular-svg-icons": "^6.3.0",
        "@fortawesome/free-solid-svg-icons": "^6.3.0",
        "@fortawesome/vue-fontawesome": "^3.0.3",
        "@lastgameboard/boardservice-client": "^0.3.8",
        "@planarally/dice": "^0.5.0",
        "mathjs": "^11.5.1",
        "path-data-polyfill": "^1.0.4",
        "socket.io-client": "^4.6.0",
        "swiper": "^7.3.1",
        "tinycolor2": "^1.6.0",
        "vue": "^3.2.47",
        "vue-i18n": "9.2.2",
        "vue-router": "^4.1.6",
        "vue-toastification": "^2.0.0-rc.5",
        "vue3-markdown-it": "^1.0.10",
        "vuedraggable": "4.0.3"
    },
    "devDependencies": {
        "@babylonjs/core": "^4.2.2",
        "@esbuild-plugins/esm-externals": "^0.1.2",
        "@intlify/unplugin-vue-i18n": "^0.8.2",
        "@types/lodash": "^4.14.191",
        "@types/swiper": "^5.4.3",
        "@types/tinycolor2": "^1.4.3",
        "@typescript-eslint/eslint-plugin": "^5.52.0",
        "@typescript-eslint/parser": "^5.52.0",
        "@vitejs/plugin-vue": "^4.0.0",
        "@vitest/coverage-c8": "^0.28.5",
        "c8": "^7.12.0",
        "dependency-cruiser": "^12.9.0",
        "eslint": "^8.34.0",
        "eslint-config-prettier": "^8.6.0",
        "eslint-import-resolver-typescript": "^3.5.3",
        "eslint-plugin-import": "^2.27.5",
        "eslint-plugin-vue": "^9.9.0",
        "happy-dom": "^8.4.2",
        "json-schema-to-typescript": "^11.0.2",
        "prettier": "^2.8.4",
        "rimraf": "^4.1.2",
        "rollup-plugin-visualizer": "^5.9.0",
        "sass": "^1.58.1",
        "typescript": "^4.9.5",
        "upath": "^2.0.1",
        "v-lazy-show": "^0.2.0",
        "vite": "^4.4.7",
        "vite-plugin-ejs": "^1.6.4",
        "vitest": "^0.33.0",
        "vue-eslint-parser": "^9.1.0",
        "vue-tsc": "^1.8.8"
    },
    "madge": {
        "detectiveOptions": {
            "ts": {
                "skipTypeImports": true
            }
        }
    }
}
