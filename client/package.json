{
    "name": "client",
    "version": "2026.1.1",
    "private": true,
    "type": "module",
    "scripts": {
        "dev": "vite",
        "serve": "vite preview",
        "clean": "node -e 'fs.rmSync(\"../server/static/vite\", { recursive: true, force: true })'",
        "build:prep": "npm run clean",
        "build": "npm run build:prep && vite build && mv ../server/index.html ../server/templates/index.html",
        "build:ci": "npm run build:prep && npm run vue-tsc && vite build --outDir dist",
        "format": "oxfmt",
        "format:ci": "oxfmt --check",
        "lint": "oxlint --fix --type-aware",
        "lint:ci": "oxlint --type-aware --deny-warnings",
        "vue-tsc": "vue-tsc --noEmit",
        "tsc": "tsc --noEmit",
        "test": "vitest",
        "coverage": "vitest run --coverage"
    },
    "dependencies": {
        "@babylonjs/core": "^8.22.2",
        "@babylonjs/materials": "^8.22.2",
        "@fortawesome/fontawesome-svg-core": "^7.2.0",
        "@fortawesome/free-brands-svg-icons": "^7.2.0",
        "@fortawesome/free-regular-svg-icons": "^7.2.0",
        "@fortawesome/free-solid-svg-icons": "^7.2.0",
        "@fortawesome/vue-fontawesome": "^3.1.3",
        "@planarally/dice": "^0.8.0",
        "@vueuse/core": "^14.2.1",
        "mathjs": "^15.1.1",
        "path-data-polyfill": "^1.0.10",
        "socket.io-client": "^4.8.3",
        "swiper": "^7.3.1",
        "tinycolor2": "^1.6.0",
        "vue": "^3.5.28",
        "vue-draggable-plus": "^0.6.1",
        "vue-i18n": "11.2.8",
        "vue-markdown-render": "^2.3.0",
        "vue-router": "^5.0.2",
        "vue-toastification": "^2.0.0-rc.5"
    },
    "devDependencies": {
        "@esbuild-plugins/esm-externals": "^0.1.2",
        "@intlify/unplugin-vue-i18n": "^11.0.3",
        "@types/lodash": "^4.17.23",
        "@types/markdown-it": "^14.1.2",
        "@types/swiper": "^5.4.3",
        "@types/tinycolor2": "^1.4.6",
        "@vitejs/plugin-vue": "^6.0.4",
        "@vitest/coverage-v8": "^4.0.18",
        "bufferutil": "^4.1.0",
        "c8": "^10.1.3",
        "happy-dom": "^20.6.1",
        "json-schema-to-typescript": "^15.0.4",
        "oxfmt": "^0.32.0",
        "oxlint": "^1.47.0",
        "oxlint-tsgolint": "^0.12.2",
        "rollup-plugin-visualizer": "^6.0.5",
        "sass": "^1.97.3",
        "typescript": "^5.9.3",
        "upath": "^2.0.1",
        "v-lazy-show": "^0.3.0",
        "vite": "^7.3.1",
        "vite-plugin-ejs": "^1.7.0",
        "vite-plugin-vue-devtools": "^8.0.6",
        "vitest": "^4.0.18",
        "vue-tsc": "^3.2.4"
    }
}
