{
    "name": "client",
    "version": "2024.2.0",
    "private": true,
    "type": "module",
    "scripts": {
        "dev": "vite",
        "serve": "vite preview",
        "clean": "rimraf ../server/static/vite/",
        "build:prep": "npm run clean",
        "build": "npm run build:prep && vite build && mv ../server/index.html ../server/templates/index.html",
        "build:ci": "npm run build:prep && npm run vue-tsc && vite build --outDir dist",
        "lint": "eslint src --fix --ext .ts,.vue",
        "lint:ci": "CI=1 eslint src --no-fix --ext .ts,.vue",
        "vue-tsc": "vue-tsc --noEmit",
        "tsc": "tsc --noEmit",
        "test": "vitest",
        "coverage": "vitest run --coverage"
    },
    "dependencies": {
        "@babylonjs/materials": "^4.2.2",
        "@fortawesome/fontawesome-svg-core": "^6.5.2",
        "@fortawesome/free-brands-svg-icons": "^6.5.2",
        "@fortawesome/free-regular-svg-icons": "^6.5.2",
        "@fortawesome/free-solid-svg-icons": "^6.5.2",
        "@fortawesome/vue-fontawesome": "^3.0.6",
        "@planarally/dice": "^0.5.0",
        "mathjs": "^12.4.2",
        "path-data-polyfill": "^1.0.4",
        "socket.io-client": "^4.7.5",
        "swiper": "^7.3.1",
        "tinycolor2": "^1.6.0",
        "vue": "^3.4.25",
        "vue-i18n": "9.13.1",
        "vue-markdown-render": "^2.1.1",
        "vue-router": "^4.3.2",
        "vue-toastification": "^2.0.0-rc.5",
        "vuedraggable": "4.0.3"
    },
    "devDependencies": {
        "@babylonjs/core": "^4.2.2",
        "@esbuild-plugins/esm-externals": "^0.1.2",
        "@intlify/unplugin-vue-i18n": "^4.0.0",
        "@types/lodash": "^4.17.0",
        "@types/swiper": "^5.4.3",
        "@types/tinycolor2": "^1.4.6",
        "@typescript-eslint/eslint-plugin": "^7.7.1",
        "@typescript-eslint/parser": "^7.7.1",
        "@vitejs/plugin-vue": "^5.0.4",
        "@vitest/coverage-v8": "^1.5.2",
        "bufferutil": "^4.0.8",
        "c8": "^9.1.0",
        "dependency-cruiser": "^16.3.1",
        "eslint": "^8.57.0",
        "eslint-config-prettier": "^9.1.0",
        "eslint-import-resolver-typescript": "^3.6.1",
        "eslint-plugin-import": "^2.29.1",
        "eslint-plugin-vue": "^9.25.0",
        "happy-dom": "^14.7.1",
        "json-schema-to-typescript": "^14.0.0",
        "prettier": "^3.2.5",
        "rimraf": "^5.0.5",
        "rollup-plugin-visualizer": "^5.12.0",
        "sass": "^1.75.0",
        "typescript": "^5.4.5",
        "upath": "^2.0.1",
        "v-lazy-show": "^0.2.4",
        "vite": "^5.2.10",
        "vite-plugin-ejs": "^1.7.0",
        "vite-plugin-vue-devtools": "^7.1.3",
        "vitest": "^1.5.2",
        "vue-eslint-parser": "^9.4.2",
        "vue-tsc": "^2.0.14"
    },
    "madge": {
        "detectiveOptions": {
            "ts": {
                "skipTypeImports": true
            }
        }
    }
}