{
    "name": "client",
    "version": "2022.2.3",
    "private": true,
    "scripts": {
        "dev": "vite",
        "serve": "vite preview",
        "clean": "rimraf ../server/static/vite/",
        "build:prep": "npm run clean",
        "build": "npm run build:prep && vite build && mv ../server/index.html ../server/templates/index.html",
        "build:ci": "npm run build:prep && npm run vue-tsc && vite build --outDir dist",
        "lint": "eslint src --fix --ext .ts,.vue",
        "lint:ci": "CI=1 eslint src --no-fix --ext .ts,.vue",
        "vue-tsc": "vue-tsc --noEmit",
        "tsc": "tsc --noEmit",
        "test": "vitest",
        "coverage": "vitest run --coverage"
    },
    "dependencies": {
        "@babylonjs/materials": "^4.2.2",
        "@fortawesome/fontawesome-svg-core": "^6.2.1",
        "@fortawesome/free-brands-svg-icons": "^6.2.1",
        "@fortawesome/free-regular-svg-icons": "^6.2.1",
        "@fortawesome/free-solid-svg-icons": "^6.2.1",
        "@fortawesome/vue-fontawesome": "^3.0.2",
        "@planarally/dice": "^0.4.0-alpha.7",
        "mathjs": "^11.3.3",
        "path-data-polyfill": "^1.0.4",
        "socket.io-client": "^4.5.3",
        "swiper": "^7.3.1",
        "tinycolor2": "^1.4.2",
        "vue": "^3.2.45",
        "vue-i18n": "9.1.10",
        "vue-router": "^4.1.6",
        "vue-toastification": "^2.0.0-rc.5",
        "vue3-markdown-it": "^1.0.10",
        "vuedraggable": "4.0.3"
    },
    "devDependencies": {
        "@babylonjs/core": "^4.2.2",
        "@intlify/vite-plugin-vue-i18n": "^5.0.1",
        "@types/lodash": "^4.14.189",
        "@types/swiper": "^5.4.3",
        "@types/tinycolor2": "^1.4.3",
        "@typescript-eslint/eslint-plugin": "^5.43.0",
        "@typescript-eslint/parser": "^5.43.0",
        "@vitejs/plugin-vue": "^3.2.0",
        "@vitest/coverage-c8": "^0.25.2",
        "c8": "^7.12.0",
        "dependency-cruiser": "^11.18.0",
        "eslint": "^8.27.0",
        "eslint-import-resolver-typescript": "^3.5.2",
        "eslint-plugin-import": "^2.26.0",
        "eslint-plugin-prettier": "^4.2.1",
        "eslint-plugin-vue": "^9.7.0",
        "happy-dom": "^7.7.0",
        "prettier": "^2.7.1",
        "rimraf": "^3.0.2",
        "rollup-plugin-visualizer": "^5.8.3",
        "sass": "^1.56.1",
        "typescript": "^4.9.3",
        "upath": "^2.0.1",
        "vite": "^3.2.4",
        "vitest": "^0.25.2",
        "vue-tsc": "^1.0.9"
    },
    "madge": {
        "detectiveOptions": {
            "ts": {
                "skipTypeImports": true
            }
        }
    }
}
