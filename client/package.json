{
    "name": "client",
    "version": "2026.1.2",
    "private": true,
    "type": "module",
    "scripts": {
        "dev": "vite",
        "serve": "vite preview",
        "clean": "node -e 'fs.rmSync(\"../server/static/vite\", { recursive: true, force: true })'",
        "build:prep": "npm run clean",
        "build": "npm run build:prep && vite build && mv ../server/index.html ../server/templates/index.html",
        "build:ci": "npm run build:prep && npm run vue-tsc && vite build --outDir dist",
        "format": "prettier --write src",
        "format:ci": "prettier --check src",
        "lint": "eslint src --fix --ext .ts,.vue",
        "lint:ci": "CI=1 eslint src --no-fix --ext .ts,.vue",
        "vue-tsc": "vue-tsc --noEmit",
        "tsc": "tsc --noEmit",
        "test": "vitest",
        "coverage": "vitest run --coverage"
    },
    "dependencies": {
        "@babylonjs/core": "^8.22.2",
        "@babylonjs/materials": "^8.22.2",
        "@fortawesome/fontawesome-svg-core": "^7.1.0",
        "@fortawesome/free-brands-svg-icons": "^7.1.0",
        "@fortawesome/free-regular-svg-icons": "^7.1.0",
        "@fortawesome/free-solid-svg-icons": "^7.1.0",
        "@fortawesome/vue-fontawesome": "^3.1.2",
        "@planarally/dice": "^0.8.0",
        "@vueuse/core": "^14.1.0",
        "mathjs": "^15.1.0",
        "path-data-polyfill": "^1.0.10",
        "socket.io-client": "^4.8.1",
        "swiper": "^7.3.1",
        "tinycolor2": "^1.6.0",
        "vue": "^3.5.26",
        "vue-draggable-plus": "^0.6.0",
        "vue-i18n": "11.2.7",
        "vue-markdown-render": "^2.3.0",
        "vue-router": "^4.6.4",
        "vue-toastification": "^2.0.0-rc.5"
    },
    "devDependencies": {
        "@esbuild-plugins/esm-externals": "^0.1.2",
        "@intlify/unplugin-vue-i18n": "^11.0.3",
        "@types/lodash": "^4.17.21",
        "@types/markdown-it": "^14.1.2",
        "@types/swiper": "^5.4.3",
        "@types/tinycolor2": "^1.4.6",
        "@typescript-eslint/eslint-plugin": "^7.10.0",
        "@typescript-eslint/parser": "^7.10.0",
        "@vitejs/plugin-vue": "^6.0.3",
        "@vitest/coverage-v8": "^4.0.16",
        "bufferutil": "^4.1.0",
        "c8": "^10.1.3",
        "eslint": "^8.57.0",
        "eslint-config-prettier": "^9.1.0",
        "eslint-import-resolver-typescript": "^3.6.3",
        "eslint-plugin-import": "^2.30.0",
        "eslint-plugin-vue": "^9.28.0",
        "happy-dom": "^20.0.11",
        "json-schema-to-typescript": "^15.0.4",
        "prettier": "^3.5.3",
        "rollup-plugin-visualizer": "^6.0.5",
        "sass": "^1.97.1",
        "typescript": "^5.9.3",
        "upath": "^2.0.1",
        "v-lazy-show": "^0.3.0",
        "vite": "^7.3.0",
        "vite-plugin-ejs": "^1.7.0",
        "vite-plugin-vue-devtools": "^8.0.5",
        "vitest": "^4.0.16",
        "vue-eslint-parser": "^9.4.3",
        "vue-tsc": "^3.2.0"
    }
}