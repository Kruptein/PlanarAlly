{
  "name": "planarally-client",
  "version": "0.20.0",
  "description": "A companion tool for when you travel into the planes.",
  "scripts": {
    "serve": "vue-cli-service serve",
    "build": "npm run clean && vue-cli-service build --no-clean --modern",
    "lint": "vue-cli-service lint",
    "clean": "rimraf ../server/static/js/ && rimraf ../server/static/css/",
    "i18n:report": "vue-cli-service i18n:report --src './src/**/*.?(js|vue)' --locales './src/locales/**/*.json'",
    "lint:ci": "vue-cli-service lint --no-fix",
    "test": "vue-cli-service test:unit",
    "test:coverage": "vue-cli-service test:unit --coverage"
  },
  "dependencies": {
