{
    "name": "client",
    "version": "2025.1.1",
    "private": true,
    "type": "module",
    "scripts": {
        "dev": "vite",
        "serve": "vite preview",
        "clean": "node -e 'fs.rmSync(\"../server/static/vite\", { recursive: true, force: true })'",
        "build:prep": "npm run clean",
        "build": "npm run build:prep && vite build && mv ../server/index.html ../server/templates/index.html",
        "build:ci": "npm run build:prep && npm run vue-tsc && vite build --outDir dist",
        "format": "prettier --write src",
        "format:ci": "prettier --check src",
        "lint": "eslint src --fix --ext .ts,.vue",
        "lint:ci": "CI=1 eslint src --no-fix --ext .ts,.vue",
        "vue-tsc": "vue-tsc --noEmit",
        "tsc": "tsc --noEmit",
        "test": "vitest",
        "coverage": "vitest run --coverage"
    },
    "dependencies": {
        "@babylonjs/core": "^7.43.0",
        "@babylonjs/materials": "^7.43.0",
        "@fortawesome/fontawesome-svg-core": "^6.7.2",
        "@fortawesome/free-brands-svg-icons": "^6.7.2",
        "@fortawesome/free-regular-svg-icons": "^6.7.2",
        "@fortawesome/free-solid-svg-icons": "^6.7.2",
        "@fortawesome/vue-fontawesome": "^3.0.8",
        "@planarally/dice": "^0.7.1",
        "mathjs": "^14.2.1",
        "path-data-polyfill": "^1.0.6",
        "socket.io-client": "^4.8.1",
        "swiper": "^7.3.1",
        "tinycolor2": "^1.6.0",
        "vue": "^3.5.13",
        "vue-i18n": "9.14.3",
        "vue-markdown-render": "^2.2.1",
        "vue-router": "^4.5.0",
        "vue-toastification": "^2.0.0-rc.5",
        "vuedraggable": "4.0.3"
    },
    "devDependencies": {
        "@esbuild-plugins/esm-externals": "^0.1.2",
        "@intlify/unplugin-vue-i18n": "^4.0.0",
        "@types/lodash": "^4.17.15",
        "@types/markdown-it": "^14.1.2",
        "@types/swiper": "^5.4.3",
        "@types/tinycolor2": "^1.4.6",
        "@typescript-eslint/eslint-plugin": "^7.10.0",
        "@typescript-eslint/parser": "^7.10.0",
        "@vitejs/plugin-vue": "^5.2.1",
        "@vitest/coverage-v8": "^3.0.6",
        "bufferutil": "^4.0.9",
        "c8": "^10.1.3",
        "dependency-cruiser": "^16.10.0",
        "eslint": "^8.57.0",
        "eslint-config-prettier": "^9.1.0",
        "eslint-import-resolver-typescript": "^3.6.3",
        "eslint-plugin-import": "^2.30.0",
        "eslint-plugin-vue": "^9.28.0",
        "happy-dom": "^17.1.2",
        "json-schema-to-typescript": "^15.0.4",
        "prettier": "^3.3.3",
        "rollup-plugin-visualizer": "^5.14.0",
        "sass": "^1.85.0",
        "typescript": "^5.7.3",
        "upath": "^2.0.1",
        "v-lazy-show": "^0.3.0",
        "vite": "^6.2.4",
        "vite-plugin-ejs": "^1.7.0",
        "vite-plugin-vue-devtools": "^7.7.2",
        "vitest": "^3.0.6",
        "vue-eslint-parser": "^9.4.3",
        "vue-tsc": "^2.2.2"
    },
    "madge": {
        "detectiveOptions": {
            "ts": {
                "skipTypeImports": true
            }
        }
    }
}